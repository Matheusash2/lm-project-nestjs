generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model register {
  id       String @id @default(cuid())
  userName String @unique
  password String
  name     String

  @@map("registers")
}

model client {
  id          String   @id @default(cuid())
  name        String
  lastName    String
  dateOfBirth String
  cpf         String   @unique
  rg          String   @unique

  @@map("clients")
}
